{
  "name": "youtube-downloader",
  "version": "1.0.5",
  "description": "A browser extension for downloading almost any video from YouTube, right from its page.",
  "repository": "https://github.com/avi12/youtube-downloader",
  "author": "avi12 <avi6106@gmail.com>",
  "license": "MIT",
  "homepage": "https://avi12.com",
  "bugs": {
    "email": "avi6106@gmail.com"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "pack": "web-ext build --overwrite-dest --artifacts-dir dist_packed --source-dir dist --filename=youtube-downloader.zip",
    "build-pack": "pnpm build && pnpm run pack",
    "set-version": "set-json-version -f package.json dist/manifest.json -v",
    "run-browser": "web-ext run --no-config-discovery --start-url https://www.youtube.com/watch?v=PK9VPv6hY3k --source-dir dist --verbose --target",
    "run-chromium": "pnpm run-browser chromium",
    "run-firefox": "pnpm run-browser firefox-desktop"
  },
  "devDependencies": {
    "@ffmpeg/ffmpeg": "0.10.1",
    "@mdi/js": "^6.6.95",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@sveltejs/eslint-config": "github:sveltejs/eslint-config",
    "@tsconfig/svelte": "^3.0.0",
    "@types/chrome": "^0.0.180",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "build-for-browser": "github:avi12/build-for-browser",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.3.0",
    "postcss": "^8.4.5",
    "prettier": "^2.6.1",
    "prettier-plugin-svelte": "^2.5.1",
    "rollup": "^2.63.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-scss": "3.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.47.0",
    "sass-lint": "^1.13.1",
    "set-json-version": "^1.0.7",
    "stylelint": "^14.2.0",
    "stylelint-config-standard": "^24.0.0",
    "svelte": "^3.45.0",
    "svelte-dnd-action": "^0.9.14",
    "svelte-materialify": "0.3.11",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "web-ext": "^6.6.0",
    "webext-store-incompat-fixer": "^0.3.1"
  },
  "dependencies": {
    "@vue/reactivity": "^3.2.26",
    "@vue/runtime-core": "^3.2.26",
    "css-tooltip": "^0.3.4",
    "vue": "^2.6.14"
  }
}